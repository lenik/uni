#!/bin/bash
# master-file

echo "Unconfig apache2"

    a2dissite com.99jsj.lenik-pc
    a2dissite com.99jsj.mirror

    a2dismod userdir

    a2reload

function nolocal() {
    local host="$1"
    lineconf -ktm "Remove $host as localhost" \
        /etc/hosts @local::$host
}

nolocal lenik-pc.99jsj.com
nolocal   mirror.99jsj.com

exit 0
