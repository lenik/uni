#!/bin/sh

  setupdir=@DESTDIR@@setupdir@
sysconfdir=@DESTDIR@@sysconfdir@
sudoersdir=@DESTDIR@@sudoersdir@

# IMPORTANT.
chmod 440 $sudoersdir/dev

$setupdir/check-uid.sh $sysconfdir 70 99 700 999
$setupdir/check-gid.sh $sysconfdir 70 99 700 999
$setupdir/70.sh  $sysconfdir
$setupdir/700.sh $sysconfdir

setminuid 800

exit 0
