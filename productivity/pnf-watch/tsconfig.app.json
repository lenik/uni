{
    "extends": "@vue/tsconfig/tsconfig.dom.json",
    "include": [
        "env.d.ts",
        "src/**/*",
        "src/**/*.vue",
        "src2/**/*",
        "src2/**/*.vue"
    ],
    "exclude": [
        "src/**/__tests__/*",
        "src2/**/__tests__/*"
    ],
    "compilerOptions": {
        "rootDir": ".",
        "outDir": "dist",
        "declaration": true,
        "composite": true,
        "noEmit": true,
        "baseUrl": "./packages",
        "paths": {}
    }
}