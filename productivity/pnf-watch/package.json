{
    "name": "pnf-watch",
    "version": "1.0.0",
    "main": "./dist-electron/main/index.js",
    "description": "PnF watch",
    "author": "Lenik <lenik@bodz.net>",
    "license": "MIT",
    "keywords": [
        "electron",
        "rollup",
        "vite",
        "vue3",
        "vue"
    ],
    "debug": {
        "env": {
            "VITE_DEV_SERVER_URL": "http://127.0.0.1:3344/"
        }
    },
    "type": "module",
    "scripts": {
        "dev": " vite -c ./vite.config.ts --host 0.0.0.0",
        "build": "run-p type-check \"build-only {@}\" -- && electron-builder",
        "preview": "vite -c ./vite.config.ts preview",
        "test:unit": "vitest -c ./vitest.config.ts",
        "build-only": "vite -c ./vite.config.ts build",
        "type-check": "vue-tsc --build --force",
        "clean": " vite -c ./vite.config.ts clean",
        "main": "electron .",
        "start": "electron-forge start",
        "package": "electron-forge package",
        "make": "electron-forge make",
        "edev": "electron-vite dev",
        "edev-w": "electron-vite dev -w",
        "ebuild": "electron-vite build",
        "epreview": "electron-vite preview"
    },
    "dependencies": {
        "@eirslett/jquery-ui-esm": "1.12.1-alpha",
        "@fontsource/noto-sans-sc": "^5.0.17",
        "bootstrap": "^4.6.2",
        "chart.js": "^4.4.7",
        "chartjs-plugin-annotation": "^3.1.0",
        "chartjs-plugin-datalabels": "^2.2.0",
        "clipboard": "^2.0.11",
        "datatables-tabletools": "^2.2.2",
        "datatables.net": "^1.13.8",
        "datatables.net-bs": "^1.13.8",
        "datatables.net-bs5": "^1.13.8",
        "datatables.net-buttons": "^2.4.2",
        "datatables.net-buttons-bs5": "^2.4.2",
        "datatables.net-dt": "^1.13.8",
        "datatables.net-plugins": "^1.13.6",
        "datatables.net-responsive": "^2.5.0",
        "datatables.net-select": "^1.7.0",
        "datatables.net-select-dt": "^1.7.0",
        "font-awesome": "^4.7.0",
        "jquery": "^3.7.1",
        "jquery-ui": "^1.13.2",
        "jquery-ui-dist": "^1.13.2",
        "lodash-es": "^4.17.21",
        "moment": "^2.30.1",
        "pinia": "^2.1.7",
        "skel01-core": "link:../../lib/libjs-skel01/packages/core",
        "skel01-dba": "link:../../lib/libjs-skel01/packages/dba"
    },
    "devDependencies": {
        "@electron/fuses": "^1.8.0",
        "@tsconfig/node18": "^18.2.2",
        "@types/jquery": "^3.5.29",
        "@types/jsdom": "^21.1.6",
        "@types/node": "^18.19.8",
        "@vitejs/plugin-vue": "^5.0.3",
        "@vitejs/plugin-vue-jsx": "^3.1.0",
        "@vue/test-utils": "^2.4.3",
        "@vue/tsconfig": "^0.5.1",
        "electron": "^33.2.1",
        "electron-builder": "^25.1.8",
        "electron-devtools-installer": "^4.0.0",
        "install": "^0.13.0",
        "jsdom": "^23.2.0",
        "npm-run-all2": "^6.1.1",
        "sass": "^1.70.0",
        "typescript": "~5.3.3",
        "vite": "^5.0.12",
        "vite-plugin-electron": "^0.28.4",
        "vite-plugin-electron-renderer": "^0.14.6",
        "vitest": "^1.2.0",
        "vue": "^3.4.15",
        "vue-chartjs": "^5.3.2",
        "vue-draggable-resizable": "3.0.0-beta.2",
        "vue-router": "^4.2.5",
        "vue-tsc": "^1.8.27"
    },
    "rules": {
        "no-unused-vars": "off"
    }
}