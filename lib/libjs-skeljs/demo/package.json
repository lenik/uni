{
  "name": "beb23web",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "pdev": "parcel serve --dist-dir ../html.out *.html m-start/*.html",
    "pdev-f": "parcel serve --dist-dir ../html.out *.html m-start/*.html --no-cache",
    "pbuild": "parcel build --dist-dir ../html.out *.html m-start/*.html --no-optimize",
    "pbuild-f": "parcel build --dist-dir ../html.out *.html m-start/*.html --no-optimize --no-cache --no-scope-hoist",
    "dev": "vite --host 0.0.0.0",
    "build": "run-p type-check \"build-only {@}\" --",
    "preview": "vite preview",
    "test:unit": "vitest",
    "build-only": "vite build",
    "type-check": "vue-tsc --build --force"
  },
  "dependencies": {
    "chart.js": "^4.4.1",
    "clndr": "^1.5.1",
    "datatables.net": "^1.13.8",
    "datatables.net-bs": "^1.13.8",
    "datatables.net-dt": "^1.13.8",
    "datatables.net-plugins": "^1.13.6",
    "datatables.net-responsive": "^2.5.0",
    "easeljs": "^1.0.2",
    "font-awesome": "^4.7.0",
    "hammerjs": "^2.0.8",
    "iscroll": "^5.2.0",
    "jquery": "^3.7.1",
    "jquery-easing": "^0.0.1",
    "jquery-ui": "^1.13.2",
    "moment": "^2.30.1",
    "pinia": "^2.1.7",
    "preloadjs": "^1.0.1",
    "requirejs": "^2.3.6",
    "skeljs": "link:..",
    "svgo": "^3.1.0",
    "swipejs": "^2.3.1",
    "tweenjs": "^1.0.2",
    "underscore": "^1.13.6",
    "velbit-fontawesome-pro": "^5.15.4",
    "vue": "^3.4.0",
    "vue-router": "^4.2.5",
    "zingtouch": "^1.0.6"
  },
  "devDependencies": {
    "@parcel/transformer-sass": "2.10.3",
    "@parcel/transformer-vue": "2.10.3",
    "@tsconfig/node18": "^18.2.2",
    "@types/jsdom": "^21.1.6",
    "@types/node": "^18.19.3",
    "@vitejs/plugin-vue": "^5.0.0",
    "@vitejs/plugin-vue-jsx": "^3.1.0",
    "@vue/test-utils": "^2.4.3",
    "@vue/tsconfig": "^0.5.1",
    "jsdom": "^23.0.1",
    "node-sass": "^9.0.0",
    "npm-run-all2": "^6.1.1",
    "parcel": "^2.10.3",
    "sass": "^1.69.6",
    "sass-loader": "^13.3.3",
    "typescript": "~5.3.3",
    "vite": "^5.0.10",
    "vitest": "^1.1.0",
    "vue-tsc": "^1.8.27"
  },
  "rules": {
    "no-unused-vars": "off"
  },
  "pnpm": {
    "overrides": {
      "rollup": "npm:@rollup/wasm-node"
    },
    "peerDependencyRules": {
      "allowedVersions": {
        "svgo": "2.8.0"
      }
    }
  }
}
