#, c-format
msgid "Error: Failed to get file stat: %s"
msgstr "无法获取文件的属性资料: %s"

#, c-format
msgid "Insufficient memory to read config file %s."
msgstr "内存不足以载入配置文件 %s。"

#, c-format
msgid "Failed to open file %s: "
msgstr "打开文件 %s 失败: "

msgid "Read incomplete"
msgstr "数据读取不完整"

msgid "Extension name isn't specified."
msgstr "扩展名未指定"

#, c-format
msgid "Format %s is already defined as %s::%s."
msgstr "格式 %s 已经被定义为 %s::%s."

#, c-format
msgid "Can't resolve format %s."
msgstr "无法解析格式 %s."

msgid "Can't resolve gate."
msgstr "无法解析关口"

#, c-format
msgid "Invalid logging type: %s"
msgstr "无效的日志类型: %s"

#, c-format
msgid "Illegal syntax in %s: %s %s."
msgstr "%s 中的语句非法：%s %s"

msgid "Program hasn't been specified yet."
msgstr "程序名称尚未指定。"

#, c-format
msgid "Warning: %s isn't configured, yet.\n"
msgstr "警告：%s 尚未被配置。\n"

#, c-format
msgid "Query execution(%s) mode for %s"
msgstr "%s: 查询 %s 的执行模式"

#, c-format
msgid "Invoke gate fn: %s::%s"
msgstr "调用关口函数：%s::%s"

#, c-format
msgid "Disabled by the gate. allow all: %s"
msgstr "关口尚未使能，全部允许：%s"

#, c-format
msgid " - The link %s is broken, pass thru forced."
msgstr " - 链接 %s 已损坏，强行绕过。"

#, c-format
msgid " - Mode of %s for [%d] %s: %x\n"
msgstr " - %s 的运行模式 (从属于 [%d] %s): %x\n"

#, c-format
msgid "%s() denied: %s"
msgstr "由 %s() 拒绝：%s"

#, c-format
msgid "Execution Blocked: %s -> %s"
msgstr "进程被拦截：%s -> %s"

#, c-format
msgid "Execution mode for %s:%s\n"
msgstr "进程 %s 的执行模式：%s\n"

#, c-format
msgid "Execution Allowed: %s -> %s"
msgstr "进程允许：%s -> %s"

#, c-format
msgid "Re-entrant is avoid: %s"
msgstr "重入避免：%s"

msgid "Library name isn't specified."
msgstr "库名称未指定。"

#, c-format
msgid "Failed to open library %s"
msgstr "无法打开库 ‘%s’"

#, c-format
msgid "No such function in library %s: %s."
msgstr "库 %s 中不存在函数 %s。"

#~ msgid "Gate returned. continue..."
#~ msgstr "关口返回。继续运行……"
