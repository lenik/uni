
BITS=4096

include @pkgdatadir@/mkcert.inc

.SECONDARY: $(*.rand)

all:
	@echo "No key file or certificate file is specified."

clean:
	rm -f *.csr-config *.csr *-x.pem
	rm -f *.spc *.pk8 *.p12
	rm -f *.pvkimprt.bat
	rm -f *.all *.w32

clean-f: clean
	rm -f *.pem
	rm -f *.csr
	rm -f *.crt

clean-ff: clean-f
	rm -f *.rand
