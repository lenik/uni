	<mirrors>
		<mirror>
			<id>mirror-releases</id>
			<name>Mirror of Releases</name>
			<mirrorOf>central</mirrorOf>
			<url>http://m2.bodz.net/archiva/repository/mirror-releases</url>
		</mirror>
		<mirror>
			<id>mirror-snapshots</id>
			<name>Mirror of Snapshots</name>
			<mirrorOf>snapshots</mirrorOf>
			<url>http://m2.bodz.net/archiva/repository/mirror-snapshots</url>
		</mirror>
	</mirrors>
	<profiles>
		<profile>
			<id>lenik-default</id>
			<properties>
				<downloadSources>true</downloadSources>
				<downloadJavadocs>true</downloadJavadocs>
			</properties>
			<repositories>
				<repository>
					<id>bodz.releases</id>
					<url>http://m2.bodz.net/archiva/repository/releases</url>
					<releases>
						<enabled>true</enabled>
					</releases>
					<snapshots>
						<enabled>false</enabled>
					</snapshots>
				</repository>
				<repository>
					<id>bodz.snapshots</id>
					<url>http://m2.bodz.net/archiva/repository/snapshots</url>
					<releases>
						<enabled>false</enabled>
					</releases>
					<snapshots>
						<enabled>true</enabled>
					</snapshots>
				</repository>
			</repositories>
		</profile>
		<profile>
			<id>plugin-snapshots</id>
			<pluginRepositories>
				<pluginRepository>
					<id>apache.snapshots</id>
					<url>http://repository.apache.org/content/groups/snapshots</url>
					<releases>
						<enabled>false</enabled>
					</releases>
					<snapshots>
						<enabled>true</enabled>
					</snapshots>
				</pluginRepository>
			</pluginRepositories>
		</profile>
	</profiles>
	<activeProfiles>
		<activeProfile>lenik-default</activeProfile>
		<activeProfile>plugin-snapshots</activeProfile>
	</activeProfiles>
</settings>
