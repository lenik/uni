move-to-workspace-left='<Shift><Mod4>h'
move-to-workspace-right='<Shift><Mod4>l'
move-to-workspace-up='<Shift><Mod4>k'
toggle-fullscreen='<Alt>F11'
toggle-on-all-workspaces='<Shift><Mod4>asterisk'
toggle-shaded='<Mod4>F12'
