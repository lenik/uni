run-command-terminal='<Mod4>t'
switch-to-workspace-down='<Mod4>j'
switch-to-workspace-left='<Mod4>h'
switch-to-workspace-right='<Mod4>l'
switch-to-workspace-up='<Mod4>k'

[org/mate/marco/window-keybindings]
